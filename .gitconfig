[user]
	name = Lattapon Yodsuwan
	email = i@clozed2u.com
[core]
	autocrlf = input
	safecrlf = true
	editor = vim
	excludesfile = ~/.gitignore
[alias]
	co = checkout
[apply]
	whitespace = nowarn
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
	ui = true
	pager = true
[credential]
	helper = osxkeychain
[push]
	default = simple 
[default]
	push = current
